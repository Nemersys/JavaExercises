package app;

import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

 
    	        for ( int i = 0 ; i < 3; i++ ) {
    	        	if (i == 2) {
    	             System.out.println("1");
    	        	}
    	             else {
    	             System.out.println("0");
    	        }
    	}
    	
    }
}