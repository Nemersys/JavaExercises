package app;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		String str = "abababbbaaba";

		
		int count = 0;
		for (int i = 0; i<str.length(); i++) {
			char c = str.charAt(i);
			
			c = Chacater.toLowerCase(c);
			
			if(c =='a' || c =='b' ) {
				count++;
			}
			
			System.out.println(count);
		}
    }
}
